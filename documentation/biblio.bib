
@unpublished{albertiLabelingCuttingGrouping2019,
  title = {Labeling, {{Cutting}}, {{Grouping}}: An {{Efficient Text Line Segmentation Method}} for {{Medieval Manuscripts}}},
  shorttitle = {Labeling, {{Cutting}}, {{Grouping}}},
  author = {Alberti, Michele and Vögtlin, Lars and Pondenkandath, Vinaychandran and Seuret, Mathias and Ingold, Rolf and Liwicki, Marcus},
  date = {2019-07-01},
  number = {arXiv:1906.11894},
  eprint = {1906.11894},
  eprinttype = {arxiv},
  primaryclass = {cs},
  publisher = {{arXiv}},
  doi = {10.48550/arXiv.1906.11894},
  url = {http://arxiv.org/abs/1906.11894},
  urldate = {2022-05-24},
  abstract = {This paper introduces a new way for text-line extraction by integrating deep-learning based pre-classification and state-of-the-art segmentation methods. Text-line extraction in complex handwritten documents poses a significant challenge, even to the most modern computer vision algorithms. Historical manuscripts are a particularly hard class of documents as they present several forms of noise, such as degradation, bleed-through, interlinear glosses, and elaborated scripts. In this work, we propose a novel method which uses semantic segmentation at pixel level as intermediate task, followed by a text-line extraction step. We measured the performance of our method on a recent dataset of challenging medieval manuscripts and surpassed state-of-the-art results by reducing the error by 80.7\%. Furthermore, we demonstrate the effectiveness of our approach on various other datasets written in different scripts. Hence, our contribution is two-fold. First, we demonstrate that semantic pixel segmentation can be used as strong denoising pre-processing step before performing text line extraction. Second, we introduce a novel, simple and robust algorithm that leverages the high-quality semantic segmentation to achieve a text-line extraction performance of 99.42\% line IU on a challenging dataset.},
  archiveprefix = {arXiv}
}

@inproceedings{antonacopoulosICDAR2015CompetitionRecognition2015,
  title = {{{ICDAR2015}} Competition on Recognition of Documents with Complex Layouts - {{RDCL2015}}},
  booktitle = {2015 13th {{International Conference}} on {{Document Analysis}} and {{Recognition}} ({{ICDAR}})},
  author = {Antonacopoulos, A. and Clausner, C. and Papadopoulos, C. and Pletschacher, S.},
  date = {2015-08},
  pages = {1151--1155},
  doi = {10.1109/ICDAR.2015.7333941},
  abstract = {This paper presents an objective comparative evaluation of page segmentation and region classification methods for documents with complex layouts. It describes the competition (modus operandi, dataset and evaluation methodology) held in the context of ICDAR2015, presenting the results of the evaluation of eight methods - four submitted, two state-of-the-art systems (one commercial and one open-source) and their two immediately previous versions. Three scenarios are reported in this paper, one evaluating the ability of methods to accurately segment regions and two evaluating both segmentation and region classification (one with emphasis on text and the other focusing only on text). The results indicate that an innovative approach has a clear advantage but there is still a considerable need to develop robust methods that deal with layout challenges, especially with the non-text content.},
  eventtitle = {2015 13th {{International Conference}} on {{Document Analysis}} and {{Recognition}} ({{ICDAR}})}
}

